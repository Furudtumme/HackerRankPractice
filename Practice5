import java.util.Scanner;
public class Solution {
    public static void main(String[] args){
    Scanner input=new Scanner(System.in);
    int num=input.nextInt();
    for(int i=1; i<=10;i++){
        int total=1;
        total= num*i;
        System.out.println(num + " x "+ i +" = " + total);
    }    
}
}
